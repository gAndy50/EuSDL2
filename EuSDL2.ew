--global SDL variable
atom sdl2

--OS check
ifdef WIN32 then
  sdl2 = open_dll("SDL2.dll")
 elsifdef LINUX or FREEBSD then
  sdl2 = open_dll("SDL2.so")
end ifdef

if sdl2 < 1 then
	puts(1, "Failed to load SDL2.dll!\n")
	abort(0)
end if

--all flag constants declared in here
include flags.e

